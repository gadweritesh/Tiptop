<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/graidentsecond"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/navigation"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="-35dp"
        android:background="@drawable/gradient_six"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:orientation="vertical">


            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:elevation="4dp"
                app:cardElevation="0dp">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <RelativeLayout
                        android:id="@+id/relative"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:background="@drawable/graidentthree">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="right|end">


                            <LinearLayout
                                android:layout_width="80dp"
                                android:layout_height="120dp"
                                android:layout_marginBottom="10dp">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/dashboardimagebook"  />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="right|end">

                        </LinearLayout>

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/dashTeacherImageView"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_centerVertical="true"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_user"
                            app:civ_border_color="@color/colorWhite"
                            app:civ_border_width="2dp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@+id/dashAdminImageView"
                            android:layout_alignBottom="@+id/dashAdminImageView"
                            android:layout_marginBottom="2dp"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="70dp"
                            android:layout_toEndOf="@+id/dashAdminImageView"
                            android:layout_toRightOf="@+id/dashAdminImageView"
                            android:orientation="vertical"
                            android:weightSum="2.1">

                            <TextView
                                android:id="@+id/name_textView"
                                style="@style/Base.TextAppearance.AppCompat.Title"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:fontFamily="sans-serif-smallcaps"
                                android:gravity="start|center_vertical"
                                android:paddingStart="5dp"
                                android:text="Admin Name"
                                android:textStyle="bold"
                                android:textColor="@color/fontcolorfull" />

                            <TextView
                                android:id="@+id/class_textView"
                                style="@style/Base.TextAppearance.AppCompat.Subhead"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:gravity="start|center_vertical"
                                android:paddingStart="5dp"
                                android:textStyle="bold"
                                android:textColor="@color/fontcolorfull" />

                        </LinearLayout>


                    </RelativeLayout>
                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <!--    <TextView
                    android:id="@+id/marqueHeadlineTextView"
                    style="@style/Base.TextAppearance.AppCompat.Medium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
