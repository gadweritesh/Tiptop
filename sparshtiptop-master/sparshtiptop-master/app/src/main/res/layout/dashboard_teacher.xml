            <!-- <TextView
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:text=""
                 android:fontFamily="thinitalic"
                 android:background="@drawable/graidentthree"
                 android:textColor="@color/blackandwhite"
                 />
